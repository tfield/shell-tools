unsetopt share_history 
echo -n "Greetings $USER! Initializing"

[ ! -d $HOME/bin ] && mkdir -p $HOME/bin
export PATH=$HOME/bin:$PATH

[ ! -f $HOME/bin/logtail.sh ]  && echo -n " logtail" && curl https://raw.githubusercontent.com/tfield/shell-tools/main/logtail.sh > $HOME/bin/logtail.sh && chmod +x logtail.sh
alias tail=logtail.sh

alias curltime='curl -w "@$HOME/Dropbox/tfield-convenience-scripts/curl-format.txt" -o /dev/null -s '
alias p='cd /Volumes/Projects'

export PATH=/usr/local/mysql/bin:$PATH

# ############################################
echo -n " oh-my-zsh"
export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="robbyrussell"
ZSH_THEME="maran"
ZSH_THEME="jnrowe"
ZSH_THEME="simple"
plugins=(git)
source $ZSH/oh-my-zsh.sh

# ############################################
echo -n ", python"
export PATH="$(pyenv root)/shims:${PATH}"

# export PATH="$HOME/.jenv/bin:$PATH"
# eval "$(jenv init -)"

# ############################################
echo -n ", nvm"
export NVM_DIR="$HOME/.nvm"
[ -s "/opt/homebrew/opt/nvm/nvm.sh" ] && \. "/opt/homebrew/opt/nvm/nvm.sh"  # This loads nvm
[ -s "/opt/homebrew/opt/nvm/etc/bash_completion.d/nvm" ] && \. "/opt/homebrew/opt/nvm/etc/bash_completion.d/nvm"  # This loads nvm bash_completion

# ############################################
# Tony: use sdkman to install multiple java versions, switch between java versions, and 
# install the micronaut command line.
echo -n ", sdkman"
#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="$HOME/.sdkman"
[[ -s "$HOME/.sdkman/bin/sdkman-init.sh" ]] && source "$HOME/.sdkman/bin/sdkman-init.sh"

# ############################################
echo "."
